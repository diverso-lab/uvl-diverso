imports
    FileSystem as fs
features
    Server { abstract }
        mandatory
            FileSystem
                [1..*]
                    NTFS
                    APFS
                    EXT4
            OperatingSystem { abstract }
                alternative
                    Windows
                    macOS
                    Debian
        optional
            Logging {
            default ,
            log_level " warn "
        }
constraints
    Windows => NTFS
    macOS => APFS
